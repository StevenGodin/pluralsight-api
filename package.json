{
  "name": "pluralsight",
  "version": "1.0.2",
  "description": "Pluralsight API NodeJS library with full support of all the Pluralsight API services.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "coverage": "jest && codecov",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/StevenGodin/pluralsight-api.git"
  },
  "keywords": [
    "pluralsight",
    "api",
    "nodejs"
  ],
  "author": "Steven Godin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/StevenGodin/pluralsight-api/issues"
  },
  "homepage": "https://github.com/StevenGodin/pluralsight-api#readme",
  "devDependencies": {
    "@types/jest": "^25.1.4",
    "@types/neat-csv": "^4.0.0",
    "codecov": "^3.5.0",
    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "ts-jest": "^24.0.2",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.5.2"
  },
  "dependencies": {
    "cross-fetch": "^3.0.4",
    "neat-csv": "^5.0.0"
  }
}
